<p id="notice"><%= notice %></p>

<h1>Product Watcheds</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Product</th>
      <th>Deleted at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_watcheds.each do |product_watched| %>
      <tr>
        <td><%= product_watched.user %></td>
        <td><%= product_watched.product %></td>
        <td><%= product_watched.deleted_at %></td>
        <td><%= link_to 'Show', product_watched %></td>
        <td><%= link_to 'Edit', edit_product_watched_path(product_watched) %></td>
        <td><%= link_to 'Destroy', product_watched, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Watched', new_product_watched_path %>
