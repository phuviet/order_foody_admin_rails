<div class="row">
  <div class="col-md-12">
    <div class="x_panel">
      <div class="x_title">
        <h2>Products Seller</h2>
        <ul class="nav navbar-right panel_toolbox">
          <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
          </li>
          <li><a class="close-link"><i class="fa fa-close"></i></a>
          </li>
        </ul>
        <div class="clearfix"></div>
        <div class="x_content">
          <div style="margin-top: 1%; margin-left: 1%; " class="form-inline">
              Months:
              <select class="form-control" style="width: 5%;" id="month">
                <option value="01">1</option>
                <option value="02">2</option>
                <option value="03">3</option>
                <option value="04">4</option>
                <option value="05">5</option>
                <option value="06">6</option>
                <option value="07">7</option>
                <option value="08">8</option>
                <option value="09">9</option>
                <option value="10">10</option>
                <option value="11">11</option>
                <option value="12">12</option>
              </select>
              Year:
              <input type="text" class="form-control" style="width: 8%;" value="<%= @year %>" placeholder="" id="year">
              <input class="btn btn-primary" style="margin: 1%;" type="button" value="Check"
              id="btn-check">
          </div>
          <div id="graph_bar_group" style="width:100%; height:auto;"></div>
        </div>
    </div>
  </div>
</div>
<%= javascript_include_tag 'echart/echarts-all-english-v2' %>
<%= javascript_include_tag 'echart/green.js' %>
<%= javascript_include_tag params[:controller] %>